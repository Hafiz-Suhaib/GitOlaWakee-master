
@{
    ViewData["Title"] = "CreateLawyer";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<div class="app-page-title">
    <div class="page-title-wrapper">
        <div class="page-title-heading">
            <div class="page-title-icon">
                <i class="fa fa-gavel icon-gradient bg-happy-fisher"> </i>
            </div>
            <div>
                Lawyer
                @*<div class="page-title-subheading">
                    Lets Get Started With Basics.</div>*@

            </div>
        </div>

    </div>
</div>
<script src="~/Angular/Controllers/LawyerCtrl.js">
</script>
<div class="tab-content" id="LawyerCtrl" ng-controller="LawyerCtrl">
    <div class="tab-pane tabs-animation fade show active" id="tab-content-0" role="tabpanel">
        <div class="row">
            <div class="col-lg-12 col-md-12">
                <div class="main-card mb-3 card">
                    <div class="card-body">
                        <div id="smartwizard">
                            <ul class="forms-wizard">
                                <li>
                                    <a href="#step-1">
                                        <em>1</em><span>INTRO</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#step-2">
                                        <em>2</em><span>EDUCATION</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#step-3">
                                        <em>3</em><span>LAW FIELDS</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#step-4">
                                        <em>4</em><span>ABOUT</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#step-5">
                                        <em>5</em><span>Offices</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#step-6">
                                        <em>6</em><span>PAKAGES</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="#step-7">
                                        <em>7</em><span>FINISH</span>
                                    </a>
                                </li>
                            </ul>
                            <div class="form-wizard-content">
                                <!-- Education -->
                                <div id="step-2">

                                    <div class="card">
                                        <div class="card-body">
                                            <div class="col-lg-12">
                                                <div class="main-card mb-3">
                                                    <div class="">
                                                        <br>
                                                        <h5 class="card-title">Add Your Education Detail</h5>
                                                        <table class="mb-0 table">
                                                            <thead>
                                                                <tr>
                                                                    <th>Degree Type</th>
                                                                    <th>Degree Name</th>
                                                                    <th>Specialization</th>
                                                                    <th>Degree Year</th>
                                                                    <th style="text-align:right;">
                                                                        <button ng-click="addQualification()" class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-dashed btn btn-outline-success"><i class="fa fa-fw" aria-hidden="true" title="Copy to use plus"></i></button>
                                                                        @*<button class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-outline-2x btn btn-outline-light"><i class="fa fa-fw" aria-hidden="true" title="Copy to use plus"></i></button>*@
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="quali in lawyerQualifications">

                                                                    <td ng-init="getDegreeType()">
                                                                        <select class="mb-2 form-control" ng-change="casCading($index,quali.degreeTypeId)" ng-model="quali.degreeTypeId" ng-options="degType.degreeTypeId as degType.typeName for degType in degreeTypeList">
                                                                            <option value="">Select Degree Type</option>
                                                                        </select>
                                                                    </td>
                                                                    <td ng-init="getDegree()">
                                                                        <select class="mb-2 form-control" ng-model="quali.degreeId" ng-change="validateDegree($index,quali.degreeId)" name="degreeId" ng-options="deg.degreeId as deg.name for deg in degrees[$index] ">
                                                                            <option value="">Select Degree Name</option>
                                                                        </select>
                                                                    </td>
                                                                    <td ng-init="getSpecialization()">
                                                                        <select class="mb-2 form-control" ng-disabled="quali.check" name="fieldName" ng-model="quali.specializationId" ng-options="special.specializationId as special.specializationName for special in specializationList ">
                                                                            <option value="" disabled>Select Field Name</option>
                                                                        </select>
                                                                    </td>
                                                                    <td>
                                                                        <input class="mb-2 form-control" ng-change="validateEducation()" ng-model="quali.completionYear" type="number" placeholder="eg:2020" />
                                                                    </td>
                                                                    <td style="text-align:right;"><button ng-click="removeQualification($index)" class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-dashed btn btn-outline-danger"><i class="pe-7s-trash btn-icon-wrapper"> </i></button></td>
                                                                </tr>



                                                            </tbody>
                                                        </table>
                                                    </div>

                                                </div>

                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <!--  -->
                                <div id="step-1">

                                    <div class="card">


                                        <div class="card-body">
                                            <h5 class="card-title">Let's Get Started With Basics</h5><hr />
                                            <form name="lawyerProfileForm">
                                                <div class="form-row">
                                                    <div class="col-md-4">
                                                        <div class="position-relative form-group">
                                                            <label for="exampleEmail55"><b>First Name</b></label>
                                                            <input ng-change="validateIntro()" id="fName" placeholder="Enter First Name" ng-model="addLawyerProfile.firstName" name="firstName" required type="text" class="mb-2 form-control">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="position-relative form-group">
                                                            <label for="exampleEmail55"><b>Last Name</b></label>
                                                            <input ng-change="validateIntro()" id="lName" ng-model="addLawyerProfile.lastName" required placeholder="Enter Last Name" type="text" class="mb-2 form-control">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="position-relative form-group">
                                                            <label for=""><b>Date of Birth</b></label>
                                                            <input ng-change="validateIntro()" ng-model="addLawyerProfile.dateOfBirth" id="dob" @*placeholder="mm/dd/yyyy"*@ type="date" class="form-control" @*data-toggle="datepicker"*@>
                                                        </div>
                                                    </div>

                                                </div>
                                                <div class="form-row">
                                                    <div class="col-md-4">
                                                        <label for="email"><b>Gender</b></label>
                                                        <select ng-change="validateIntro()" id="gender" class="mb-2 form-control" ng-model="addLawyerProfile.gender">
                                                            <option value="" disabled selected>Gender</option>
                                                            <option value="male">Male</option>
                                                            <option value="female">Female</option>
                                                        </select>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="position-relative form-group">
                                                            <label for="exampleEmail55"><b>Cnic</b></label>
                                                            <input ng-change="validateIntro()" id="cnic" name="cnic" ng-model="addLawyerProfile.cnic" ng-pattern="/^[0-9]{1,5}-[0-9]{1,7}-[0-9]{1}$/" placeholder="Cnic Ex: 35101-1234567-8" type="text" class="mb-2 form-control">
                                                            <span style="color:Red" ng-show="lawyerProfileForm.cnic.$error.pattern">cnic formate eg 34102-1234567-8 </span>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="position-relative form-group">
                                                            <label for="mobileno"><b>Mobile No</b></label>
                                                            <input ng-change="validateIntro()" name="contact" id="mobileno" ng-model="addLawyerProfile.contact" ng-pattern="/^[0-9]{11,11}$/" placeholder="Contact Ex: 03001234567" type="text" class="mb-2 form-control">
                                                            <span style="color:Red" ng-show="lawyerProfileForm.contact.$error.pattern">enter 11 digits Ex: 03001234567</span>
                                                        </div>

                                                    </div>
                                                </div>@{ var emailicon = "@";}
                                                <div class="form-row">
                                                    <div class="col-md-4">
                                                        <label for="email"><b>Email</b></label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <div class="input-group-text">
                                                                    @emailicon
                                                                </div>
                                                            </div>
                                                            <input ng-change="validateIntro()" class="form-control input-mask-trigger" placeholder="example@gmail.com" ng-model="addLawyerAccount.email" required id="email" @*data-inputmask="'alias': 'email'" im-insert="true"*@>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="position-relative form-group">
                                                            <label for="exampleEmail55"><b>Username</b></label>
                                                            <input ng-change="validateIntro()" id="userName" ng-model="addLawyerAccount.userName" @*ng-change="checkUserAvailable()"*@ required placeholder="Enter Username" type="text" class="mb-2 form-control">
                                                            <p class="text-danger" style="display:none" id="checkUser">this username is already exit</p>
                                                        </div>

                                                    </div>

                                                    <div class="col-md-4">
                                                        <div class="position-relative form-group">
                                                            <label for="examplePassword22"><b>Password</b></label>
                                                            <input ng-change="validateIntro()" name="password" id="password" ng-pattern="/^(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{8,20}$/" ng-model="addLawyerAccount.passwordHash" placeholder="Enter Password" type="password" class="form-control">
                                                            <span style="color:Red" ng-show="lawyerProfileForm.password.$error.pattern">[Min 8 char]atleast one special,Uppercase & digit</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="position-relative form-group">
                                                    <label for="exampleAddress"><b>Address</b></label>
                                                    <input ng-change="validateIntro()" name="address" id="autocomplete" ng-model="addLawyerProfile.address" @*onFocus="geolocate()"*@ placeholder="Search Address Ex: garden town" type="text" class="form-control">
                                                </div>
                                                <!-- <div class="position-relative form-group">
                                                    <label for="exampleAddress2">Address 2</label>
                                                    <input name="address2" id="exampleAddress2" placeholder="Apartment, studio, or floor" type="text" class="form-control">
                                                </div> -->
                                                <div class="form-row">
                                                    <div class="col-md-6">
                                                        <div class="position-relative form-group">
                                                            <label for="exampleCity"><b>City</b></label>
                                                            <input name="city" id="locality" ng-model="addLawyerProfile.city" type="text" readonly class="form-control" placeholder="Enter City">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <div class="position-relative form-group">
                                                            <label for="exampleState"><b>Country</b></label>
                                                            <input id="country" type="text" ng-model="addLawyerProfile.country" class="form-control" readonly placeholder="Enter Country">
                                                        </div>
                                                    </div>
                                                    <!-- <div class="col-md-2">
                                                        <div class="position-relative form-group">
                                                            <label for="exampleZip">Zip</label>
                                                            <input name="zip" id="exampleZip" type="text" class="form-control">
                                                        </div>
                                                    </div> -->
                                                </div>

                                                <!--  <input class="field" id="street_number" disabled="true"/>
                                                <input class="field" id="route" disabled="true"/>

                                                <input class="field" id="administrative_area_level_1" disabled="true"/>

                                                <input class="field" id="postal_code" disabled="true"/> -->




                                                <div class="form-row">
                                                    <div class="col-md-4">
                                                        <label><b>Profile Picture</b> <span class="text-danger">*</span></label>
                                                        <div class="form-group">
                                                            <input style="display:none" type="file" accept="image/*" id="fileInput" oninput="showImage()">
                                                            <div id="ImageTab" class="center">
                                                            </div>
                                                            <img style="width:40px;margin-left: 20px; margin-right:20px" id="selectImageBtn" class="icon-colored pointer" onclick="$('#fileInput').click()" src="~/assets/images/image-upload.png" title="add_image.svg">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4" ng-init="getCaseCategory()">

                                                        <label><b>Services</b></label>
                                                        <select multiple="multiple" id="GetCaseCatIds" onchange="ValidateIntro()" class="multiselect-dropdown form-control" @*ng-change="validateIntro()"*@>
                                                            @*<option value="" selected disabled>select services</option>*@
                                                            <option ng-repeat="case in caseCategoryList" value="{{case.caseCategoryId}}">{{case.name}}</option>

                                                        </select>

                                                    </div>
                                                    <div class="col-md-4">

                                                        <label><b>Add Your Languages</b></label>
                                                        <select multiple="multiple" id="GetLanguages" onchange="ValidateIntro()" class="multiselect-dropdown form-control" @*ng-change="validateIntro()"*@>
                                                            @*<option value="" selected disabled hidden>Select Language</option>*@
                                                            <option value="English">English</option>
                                                            <option value="Urdu">Urdu</option>
                                                            <option value="Punjabi">Punjabi</option>
                                                            <option value="Pashto">Pashto</option>
                                                            <option value="Saraiki">Saraiki</option>

                                                        </select>

                                                    </div>

                                                </div>
                                            </form>
                                        </div>

                                    </div>

                                </div>
                                <!-- Law Fields -->
                                <div id="step-3">

                                    <div class="position-relative form-check">
                                        <div class="mb-3 card">
                                            <div class="card-header">
                                                <ul class="nav">
                                                    <li class="nav-item">
                                                        <a data-toggle="tab" href="#tab-eg1-0" class="nav-link active">Experience</a>
                                                    </li>
                                                    <li class="nav-item">
                                                        <a data-toggle="tab" href="#tab-eg1-1" class="nav-link">Total Experience</a>
                                                    </li>
                                                    <li class="nav-item">
                                                        <a data-toggle="tab" href="#tab-eg1-2" class="nav-link">License</a>
                                                    </li>
                                                </ul>
                                            </div>
                                            <div class="card-body">
                                                <div class="tab-content">
                                                    <div class="tab-pane active" id="tab-eg1-0" role="tabpanel">
                                                        <div class="col-lg-12">
                                                            <div class="main-card mb-3">
                                                                <div class="">
                                                                    <br>
                                                                    <h5 class="card-title">Add Your Experience</h5>
                                                                    <table class="mb-0 table">
                                                                        <thead>
                                                                            <tr>

                                                                                <th width="50%">Field Name</th>
                                                                                <th width="50%">Experience Year</th>

                                                                                <th style="text-align:right;">
                                                                                    <button ng-click="addExperience()" class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-dashed btn btn-outline-success"><i class="fa fa-fw" aria-hidden="true" title="Copy to use plus"></i></button>
                                                                                    @*<button class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-outline-2x btn btn-outline-light"><i class="fa fa-fw" aria-hidden="true" title="Copy to use plus"></i></button>*@
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody>
                                                                            <tr ng-repeat="expe in lawyerExperiences">
                                                                                <td ng-init="getCaseCategory()">
                                                                                    <select class="mb-2 form-control" ng-change="validateLaw()" ng-options="case.caseCategoryId as case.name for case in caseCategoryList " ng-model="expe.caseCategoryId">
                                                                                        <option value="" disabled>Field Name</option>
                                                                                    </select>
                                                                                </td>
                                                                                <td>
                                                                                    <select class="mb-2 form-control" ng-change="validateLaw()" name="timeTo" ng-model="expe.experienceYears" ng-options="year.id as year.name for year in yearList ">
                                                                                        <option value="" disabled>Select Year</option>
                                                                                    </select>
                                                                                    @* <input placeholder="0" ng-model="expe.experienceYears" name="year" type="number" min="0" max="30" required class="mb-2 form-control">*@
                                                                                </td>
                                                                                <td style="text-align:right;"><button ng-click="removeExperience($index)" class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-dashed btn btn-outline-danger"><i class="pe-7s-trash btn-icon-wrapper"> </i></button></td>
                                                                            </tr>
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        @*<div><a style="float:right" class="mb-2 mr-2 btn-square btn btn-outline-danger">Back</a><a style="float:right" class="mb-2 mr-2 btn-square btn btn-outline-success">Next</a></div>*@
                                                    </div>
                                                    <div class="tab-pane" id="tab-eg1-1" role="tabpanel" style="height:187px">
                                                        <br /> <h6 class="card-title">Add Your Total Experience</h6><hr />
                                                        <div class="col-md-12">
                                                            <div class="position-relative form-group">
                                                                <form name="yearForm">
                                                                    <label for="exampleCustomSelect" class=""><b>Total Year Of Experience</b></label>
                                                                    @*<input placeholder="0" name="year" type="number" min="0" max="30" required class="mb-2 form-control" ng-model="addLawyerProfile.totalExperience">*@
                                                                    <select class="mb-2 form-control" ng-change="validateLaw()" name="timeTo" ng-model="addLawyerProfile.totalExperience" ng-options="year.id as year.name for year in yearList ">
                                                                        <option value="" disabled>Select Year</option>
                                                                    </select>
                                                                </form>
                                                            </div>

                                                        </div>
                                                        @*<div><a style="float:right" data-toggle="tab" href="#tab-eg1-0" class="mb-2 mr-2 btn-square btn btn-outline-danger">Back</a><a data-toggle="tab" href="#tab-eg1-2" style="float:right" class="mb-2 mr-2 btn-square btn btn-outline-success">Next</a></div>*@
                                                    </div>
                                                    <div class="tab-pane" id="tab-eg1-2" role="tabpanel">
                                                        <div class="col-lg-12">
                                                            <div class="main-card mb-3">
                                                                <div class="">
                                                                    <br>
                                                                    <h5 class="card-title">Add your License</h5>
                                                                    <table class="mb-0 table">
                                                                        <thead>
                                                                            <tr>

                                                                                <th width="30%">Select Your Practice City</th>
                                                                                <th width="33%">District Bar</th>
                                                                                <th width="33%">City Bar</th>
                                                                                <th style="text-align:right;">
                                                                                    <button ng-click="addLawyerLicense()" class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-dashed btn btn-outline-success"><i class="fa fa-fw" aria-hidden="true" title="Copy to use plus"></i></button>
                                                                                    @*<button class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-outline-2x btn btn-outline-light"><i class="fa fa-fw" aria-hidden="true" title="Copy to use plus"></i></button>*@
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody>
                                                                            <tr ng-repeat="license in lawyerLicenses">
                                                                                <td ng-init="getCityList()">
                                                                                    <select class="mb-2 form-control" ng-change="validateCity($index,license.licenseCityId)" ng-options="city.licenseCityId as city.cityName for city in cityList " ng-model="license.licenseCityId">
                                                                                        <option value="" disabled>City Name</option>
                                                                                    </select>
                                                                                </td>

                                                                                <td><input ng-model="license.districtBar" ng-change="validateLaw()" ng-disabled="license.check" placeholder="Enter License Number Ex: 090078601" type="number" class="mb-2 form-control"></td>
                                                                                <td><input ng-model="license.cityBar" ng-change="validateLaw()" ng-disabled="license.check" placeholder="Enter License Number Ex: 090078601" type="number" class="mb-2 form-control"></td>
                                                                                <td style="text-align:right;"><button ng-click="removeLawyerLicense($index)" class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-dashed btn btn-outline-danger"><i class="pe-7s-trash btn-icon-wrapper"> </i></button></td>
                                                                            </tr>
                                                                        </tbody>
                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--  <div class="d-block text-right card-footer">
                                                <button class="btn-wide btn-shadow btn btn-danger">Delete</button>
                                            </div> -->
                                        </div>

                                    </div>
                                </div>
                                <div id="step-4">

                                    <div class="mb-3 card">
                                        <div class="card-header">
                                            <ul class="nav nav-justified">
                                                <li class="nav-item">
                                                    <a data-toggle="tab" href="#tab-eg-0" class="nav-link active">Add Brief Biography</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a data-toggle="tab" href="#tab-eg-1" class="nav-link">Renowned Clients</a>
                                                </li>

                                            </ul>
                                        </div>
                                        <div class="card-body">
                                            <div class="tab-content">
                                                <div class="tab-pane show active" id="tab-eg-0" role="tabpanel">
                                                    <h5 class="card-title">Add Brief Biography</h5>
                                                    <textarea rows="1" class="form-control autosize-input" ng-change="validateAbout()" placeholder="type here...." ng-model="addLawyerProfile.biography" style="height: 150px;">Just a single line...</textarea>
                                                </div>
                                                <div class="tab-pane" id="tab-eg-1" role="tabpanel">
                                                    <!-- <h5 class="card-title">Add Brief Biography</h5> -->
                                                    <div class="main-card mb-3">
                                                        <div class="">
                                                            <br>
                                                            <h5 class="card-title">Renowned Clients</h5>
                                                            <table class="mb-0 table">
                                                                <thead>
                                                                    <tr>
                                                                        <th width="95%">Client Name</th>
                                                                        <th style="text-align:right;">
                                                                            <button ng-click="addClients()" class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-dashed btn btn-outline-success"><i class="fa fa-fw" aria-hidden="true" title="Copy to use plus"></i></button>
                                                                            @*<button class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-outline-2x btn btn-outline-light"><i class="fa fa-fw" aria-hidden="true" title="Copy to use plus"></i></button>*@
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr ng-repeat="client in lawyerClients">
                                                                        <td><input placeholder="Enter Name" ng-change="validateAbout()" ng-model="client.clientName" type="text" class="mb-2 form-control"></td>
                                                                        <td style="text-align:right;"><button ng-click="removeClient($index)" class="mb-2 mr-2 btn-icon btn-icon-only btn-shadow btn-dashed btn btn-outline-danger"><i class="pe-7s-trash btn-icon-wrapper"> </i></button></td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                    @*<div class="col-lg-12">
                                                            <div class="main-card mb-3 card">
                                                                <div class="card-body">

                                                                </div>
                                                            </div>
                                                        </div>*@
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div id="step-5">
                                    <div class="card-hover-shadow-2x mb-3 card">
                                        <div class="card-header">Add Your Office Locations</div>
                                        <div class="card-body">
                                            <div class="form-row">
                                                <div class="col-md-12">
                                                    <input @*onFocus="geolocate()"*@ id="txtPlaces" onfocus="this.value=''" placeholder="Enter address here eg: karim block" type="text" class="mb-2 form-control">
                                                </div>
                                            </div>


                                        </div>
                                        <div class="d-block text-left card-footer">
                                            @*<button class="mr-2 btn btn-link btn-sm">Cancel</button>*@
                                            @*<button class="btn-shadow-primary btn btn-primary btn-lg" onclick="document.getElementById('txtPlaces').value = ''">Clear</button>*@
                                            <button class="btn-shadow-primary btn btn-success btn-lg" ng-click="addAddress()">Add</button>
                                        </div>
                                    </div>
                                    <div class="">
                                        <h5 class="card-title">Office Locations</h5>
                                        <table class="mb-0 table">
