@model OlaWakeel.Models.Appointment

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<div class="app-page-title">
    <div class="page-title-wrapper">
        <div class="page-title-heading">
            <div class="page-title-icon">
                <i class="lnr-graduation-hat icon-gradient bg-mean-fruit"></i>
            </div>
            <div>
                Appointment
            </div>
        </div>
    </div>
</div>

<div class="main-card mb-3 card">
    <div class="card-body">
        <h5 class="card-title">Appointment Details</h5>
        <form class="">

            <div class="position-relative row form-group">
                <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.AppointmentCode)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.AppointmentCode)</label>
                </div>
            </div>
            <div class="position-relative row form-group">
                <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.LawyerAddress)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.LawyerAddress.LawyerAddressId)</label>
                </div>
            </div><div class="position-relative row form-group">
                      <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.CaseCategory)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.CaseCategory.CaseCategoryId)</label>
                </div>
            </div>
            <div class="position-relative row form-group">
                <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.TimeFrom)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.TimeFrom)</label>
                </div>
            </div>
            <div class="position-relative row form-group">
                <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.TimeTo)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.TimeTo)</label>
                </div>
            </div>
            <div class="position-relative row form-group">
                <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.CaseCharges)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.CaseCharges)</label>
                </div>
            </div>
            <div class="position-relative row form-group">
                <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.ScheduleDate)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.ScheduleDate)</label>
                </div>
            </div>
            <div class="position-relative row form-group">
                <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.Rating)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.Rating)</label>
                </div>
            </div>
            <div class="position-relative row form-group">
                <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.AppoinmentType)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.AppoinmentType)</label>
                </div>
            </div>
            <div class="position-relative row form-group">
                <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.AppoinmentStatus)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.AppoinmentStatus)</label>
                </div>
            </div>
            <div class="position-relative row form-group">
                <label for="examplePassword" class="col-sm-2 col-form-label">@Html.DisplayNameFor(model => model.Date)</label>
                <div class="col-sm-10">
                    <label for="examplePassword" class="form-control">@Html.DisplayFor(model => model.Date)</label>
                </div>
            </div>
